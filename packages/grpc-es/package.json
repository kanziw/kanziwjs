{
  "name": "@kanziw/grpc-es",
  "version": "0.0.1",
  "description": "A collection of grpc libraries using connect-es",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanziw/kanziwjs.git",
    "directory": "packages/grpc-es"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/kanziw/kanziwjs/tree/main/packages/grpc-es#readme",
  "keywords": [
    "grpc",
    "connect-es",
    "bufbuild-es",
    "protobuf-es",
    "protoc-gen-es",
    "kanziw"
  ],
  "author": {
    "name": "Jiwoong Jung",
    "email": "kanziwoong@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.js",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.js",
        "default": "./lib/cjs/index.js"
      }
    },
    "./server": {
      "import": {
        "types": "./lib/esm/server/index.d.js",
        "default": "./lib/esm/server/index.js"
      },
      "require": {
        "types": "./lib/cjs/server/index.d.js",
        "default": "./lib/cjs/server/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "./lib/esm/index.d.js",
  "main": "./lib/cjs/index.js",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "generate:example-stubs": "buf generate",
    "run:example": "tsx watch --clear-screen=false example/server.ts",
    "clean": "rimraf lib",
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:esm": "yarn tsc --noEmit false --module es2022 --outDir lib/esm",
    "build:cjs": "yarn tsc --noEmit false --module commonjs --outDir lib/cjs",
    "publish": "yarn build && yarn publish-if-not-published"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.28.1",
    "@bufbuild/protoc-gen-es": "^1.6.0",
    "@connectrpc/protoc-gen-connect-es": "^1.2.0",
    "publish-if-not-published": "^3.1.3",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "license": "MIT",
  "dependencies": {
    "@bufbuild/protobuf": "^1.6.0",
    "@connectrpc/connect": "^1.2.0",
    "@connectrpc/connect-node": "^1.2.0"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^1.6.0",
    "@connectrpc/connect": "^1.2.0",
    "@connectrpc/connect-node": "^1.2.0"
  }
}
